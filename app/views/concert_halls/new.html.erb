new.html.erb

<h1> Create A New Venue Below: </h1>

<%= form_for @concert_hall do |f| %>
	<%= f.text_field :name, placeholder: "Name" %></br>
	<%= f.text_field :city, placeholder: "City" %></br>
	<%= f.text_field :state, placeholder: "State" %></br>
	<%= f.label(:family_friendly, "Family Friendly?") %>
	<%= f.check_box(:family_friendly) %> </br>
	<%= f.submit %>

<% end %>